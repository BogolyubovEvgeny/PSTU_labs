import random
n = random.randint(1, 100)
print("Загаданное число от 1 до 100""\n""На отгадывание дается 6 попыток")
k = 0
while k != 6:
    x = int(input("Введите число"))
    if n == x:
        print("Поздравляем, вы нашли загаданное число!!!")
    elif n > x:
        k += 1
        print("Загаданное число больше")
    else:
        k += 1
        print("Загаданное число меньше")
if k == 6:
    print("Увы, попытки закончились""\n""Число =", n)
